fA;AAgBA;AAlBA;AAbA,CAAA","sourcesContent":["<template>\n<div id=\"app\">\n  <v-app id=\"inspire\">\n    <v-card\n      max-width=\"450\"\n      class=\"mx-auto\"\n    >\n      <v-toolbar\n        color=\"cyan\"\n        dark\n      >\n  \n        <v-toolbar-title>Inbox</v-toolbar-title>\n  \n        <v-spacer></v-spacer>\n  \n      </v-toolbar>\n      <div v-if=\"loading\">\n        <div class=\"text-center\">\n          <v-progress-circular\n            indeterminate\n            color=\"primary\"\n          ></v-progress-circular>\n        </div>\n      </div>\n      <div v-else>  \n        <div v-if=\"errored\">\n          {{ errored }}\n        </div>\n        <div v-else>\n          <v-list three-line>\n            <template v-for=\"(item, index) in users\">\n              <v-subheader\n                v-if=\"item.header\"\n                :key=\"item.header\"\n                v-text=\"item.header\"\n              ></v-subheader>\n      \n              <v-divider\n                v-else-if=\"item.divider\"\n                :key=\"index\"\n                :inset=\"item.inset\"\n              ></v-divider>\n      \n              <v-list-item\n                v-else\n                :key=\"item.title\"\n              >\n                <v-list-item-avatar>\n                  <v-img :src=\"item.avatar\"></v-img>\n                </v-list-item-avatar>\n      \n                <v-list-item-content>\n                  <a v-bind:href=\"item.url\">\n                    <v-list-item-title v-html=\"item.login\"></v-list-item-title>\n                  </a>\n                </v-list-item-content>\n              </v-list-item>\n            </template>\n          </v-list>\n        </div>\n      </div>\n      \n    </v-card>\n  </v-app>\n</div>\n</template>\n\n<script>\n\nexport default {\n  name: 'App',\n  created() {\n    this.getUsers()\n  },\n  mounted () {\n    this.getUsers()\n  },\n  data: () => ({\n    users: [],\n    errored: false,\n    loading: true\n  }),\n  methods: {\n    getUsers () {\n      this.axios.get('http://localhost:3000')\n      .then(response => (\n        this.loading = false,\n        this.users = response.data.items.map(function (item) {\n          return {\n            avatar:item.avatar_url, \n            login: item.login,\n            url:  item.url\n          }\n        }),\n        console.log(this.users)\n      ))\n      .catch(error => {\n        this.errored = error\n      })\n      .finally(() => this.loading = false)\n    }\n  }\n}\n</script>\n\n<style scoped>\n  .v-progress-circular {\n    margin: 1rem;\n  }\n</style>"],"sourceRoot":"src"}]}